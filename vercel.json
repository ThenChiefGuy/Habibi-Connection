{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build",
          "forceYarn": true,
          "installCommand": "yarn install --frozen-lockfile --ignore-scripts",
          "buildCommand": "yarn build"
        }
      }
    ],
    "build": {
      "env": {
        "NODE_VERSION": "18.17.1",
        "YARN_VERSION": "1.22.22"
      }
    }
  }